[core]
  editor = vim
[merge]
  tool = vimdiff
[color]
  status = auto
  branch = auto
  interactive = auto
  diff = auto
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  last = log -1 HEAD
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
